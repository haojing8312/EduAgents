{
  "dashboard": {
    "id": null,
    "title": "PBL智能助手系统监控",
    "tags": ["pbl", "monitoring", "ai-agents"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "系统概览",
        "type": "stat",
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{job=\"pbl-system\"}",
            "legendFormat": "系统状态",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "CPU使用率",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "system_cpu_usage_percent",
            "legendFormat": "CPU使用率 (%)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 60},
                {"color": "red", "value": 80}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "内存使用率",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 4},
        "targets": [
          {
            "expr": "system_memory_usage_percent",
            "legendFormat": "内存使用率 (%)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 85}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "磁盘使用率",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 4},
        "targets": [
          {
            "expr": "system_disk_usage_percent",
            "legendFormat": "磁盘使用率 (%)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "API请求速率",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "rate(api_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0
          }
        }
      },
      {
        "id": 6,
        "title": "API响应时间",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95响应时间",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P50响应时间",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0
          }
        }
      },
      {
        "id": 7,
        "title": "错误率",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "rate(api_requests_total{status=~\"4..|5..\"}[5m]) / rate(api_requests_total[5m]) * 100",
            "legendFormat": "错误率",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "智能体状态",
        "type": "stat",
        "gridPos": {"h": 6, "w": 18, "x": 6, "y": 20},
        "targets": [
          {
            "expr": "agent_requests_total",
            "legendFormat": "{{agent_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "mappings": [
              {
                "type": "value",
                "value": "0",
                "text": "离线"
              }
            ]
          }
        }
      },
      {
        "id": 9,
        "title": "智能体响应时间",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 26},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_response_time_seconds_bucket[5m]))",
            "legendFormat": "{{agent_type}} P95",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0
          }
        }
      },
      {
        "id": 10,
        "title": "智能体协作持续时间",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 26},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_collaboration_duration_seconds_bucket[5m]))",
            "legendFormat": "{{collaboration_type}} P95",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0
          }
        }
      },
      {
        "id": 11,
        "title": "数据库连接数",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 34},
        "targets": [
          {
            "expr": "database_connections_active",
            "legendFormat": "活跃连接数",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0
          }
        }
      },
      {
        "id": 12,
        "title": "数据库查询时间",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 34},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(database_query_duration_seconds_bucket[5m]))",
            "legendFormat": "{{query_type}} P95",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0
          }
        }
      },
      {
        "id": 13,
        "title": "WebSocket连接数",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 34},
        "targets": [
          {
            "expr": "websocket_connections_total",
            "legendFormat": "WebSocket连接",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 500},
                {"color": "red", "value": 1000}
              ]
            }
          }
        }
      },
      {
        "id": 14,
        "title": "WebSocket消息速率",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 40},
        "targets": [
          {
            "expr": "rate(websocket_messages_total[5m])",
            "legendFormat": "{{type}} {{direction}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "msgps",
            "min": 0
          }
        }
      },
      {
        "id": 15,
        "title": "系统告警",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 48},
        "targets": [
          {
            "expr": "ALERTS{alertstate=\"firing\"}",
            "legendFormat": "",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "color-background"
            }
          }
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "job": true,
                "instance": true
              },
              "indexByName": {},
              "renameByName": {
                "alertname": "告警名称",
                "alertstate": "状态",
                "severity": "严重级别"
              }
            }
          }
        ]
      },
      {
        "id": 16,
        "title": "TOP API端点",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 56},
        "targets": [
          {
            "expr": "topk(10, sum by (endpoint) (rate(api_requests_total[5m])))",
            "legendFormat": "",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "renameByName": {
                "endpoint": "API端点",
                "Value": "请求速率 (req/s)"
              }
            }
          }
        ]
      },
      {
        "id": 17,
        "title": "智能体使用统计",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 56},
        "targets": [
          {
            "expr": "sum by (agent_type) (agent_requests_total)",
            "legendFormat": "{{agent_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "displayLabels": ["name", "value"]
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up, instance)",
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "includeAll": true,
          "multi": true
        },
        {
          "name": "agent_type",
          "type": "query",
          "query": "label_values(agent_requests_total, agent_type)",
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "部署",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(up[1m]) > 0",
          "iconColor": "blue",
          "step": "60s",
          "tagKeys": "instance",
          "titleFormat": "服务重启",
          "textFormat": "实例 {{instance}} 重启"
        },
        {
          "name": "告警",
          "datasource": "prometheus",
          "enable": true,
          "expr": "ALERTS{alertstate=\"firing\"}",
          "iconColor": "red",
          "step": "60s",
          "tagKeys": "alertname",
          "titleFormat": "{{alertname}}",
          "textFormat": "严重级别: {{severity}}"
        }
      ]
    },
    "links": [
      {
        "title": "系统文档",
        "url": "https://docs.pbl-assistant.com",
        "type": "link",
        "icon": "doc",
        "tooltip": "查看系统文档"
      },
      {
        "title": "Sentry错误监控",
        "url": "https://sentry.io/organizations/your-org/projects/pbl-assistant/",
        "type": "link",
        "icon": "bolt",
        "tooltip": "查看错误日志"
      }
    ]
  },
  "overwrite": true
}